#include <stdio.h>
#include <string.h>

int strStr(char haystack[], char needle[]) {
    int n = strlen(haystack);
    int m = strlen(needle);
    int i, j;

    if(m == 0) return 0;

    for(i = 0; i <= n - m; i++) {
        for(j = 0; j < m; j++) {
            if(haystack[i + j] != needle[j])
                break;
        }
        if(j == m)
            return i;
    }
    return -1;
}

int main() {
    char haystack[100], needle[100];
    printf("Enter haystack string: ");
    scanf("%s", haystack);
    printf("Enter needle string: ");
    scanf("%s", needle);

    printf("Output: %d", strStr(haystack, needle));
    return 0;
}
