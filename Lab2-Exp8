#include <stdio.h>
#include <string.h>

int isSubstring(char *a, char *b) {
    int n = strlen(a), m = strlen(b);
    if(n > m) return 0;
    for(int i = 0; i <= m - n; i++) {
        int j;
        for(j = 0; j < n; j++)
            if(b[i + j] != a[j])
                break;
        if(j == n)
            return 1;
    }
    return 0;
}

