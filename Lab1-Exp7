#include <stdio.h>

int main() {
    int n, i, j, k = 0;
    int arr[100], unique[100];
    int isDuplicate;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    for(i = 0; i < n; i++) {
        isDuplicate = 0;
        for(j = 0; j < k; j++) {
            if(arr[i] == unique[j]) {
                isDuplicate = 1;
                break;
            }
        }
        if(!isDuplicate)
            unique[k++] = arr[i];
    }

    printf("Unique elements: ");
    for(i = 0; i < k; i++)
        printf("%d ", unique[i]);
    
    return 0;
}
