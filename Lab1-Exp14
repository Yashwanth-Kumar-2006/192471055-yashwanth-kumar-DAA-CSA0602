#include <stdio.h>

int uniquePaths(int m, int n) {
    int dp[100][100];  
    for(int i = 0; i < m; i++)
        dp[i][0] = 1;
    for(int j = 0; j < n; j++)
        dp[0][j] = 1;
    for(int i = 1; i < m; i++)
        for(int j = 1; j < n; j++)
            dp[i][j] = dp[i-1][j] + dp[i][j-1];

    return dp[m-1][n-1];  // bottom-right cell
}

int main() {
    int m, n;
    printf("Enter grid size (m n): ");
    scanf("%d %d", &m, &n);

    printf("Number of unique paths: %d\n", uniquePaths(m, n));
    return 0;
}
