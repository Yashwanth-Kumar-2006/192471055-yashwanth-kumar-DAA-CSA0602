#include <stdio.h>

void selectionSort(int arr[], int n) {
    int i, j, min, temp;
    for(i = 0; i < n-1; i++) {
        min = i;
        for(j = i+1; j < n; j++)
            if(arr[j] < arr[min])
                min = j;
        temp = arr[i];
        arr[i] = arr[min];
        arr[min] = temp;
    }
}

int main() {
    int arr[] = {5, 2, 9, 1, 5, 6};
    int n = 6, i;
    selectionSort(arr, n);
    printf("Sorted Array: ");
    for(i=0; i<n; i++)
        printf("%d ", arr[i]);
    return 0;
}
