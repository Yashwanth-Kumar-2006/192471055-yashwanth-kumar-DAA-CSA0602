#include <stdio.h>

int findKthMissing(int arr[], int n, int k) {
    int expected = 1, i = 0;
    while(k > 0) {
        if(i < n && arr[i] == expected)
            i++;
        else
            k--;
        if(k == 0)
            return expected;
        expected++;
    }
    return -1;
}

int main() {
    int arr[100], n, k, i;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter sorted elements: ");
    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter k: ");
    scanf("%d", &k);

    printf("Kth missing number: %d", findKthMissing(arr, n, k));
    return 0;
}
