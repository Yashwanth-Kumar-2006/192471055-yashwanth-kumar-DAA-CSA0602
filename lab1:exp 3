#include <stdio.h>

int main() {
    int n;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int nums[n];
    printf("Enter elements: ");
    for(int i = 0; i < n; i++)
        scanf("%d", &nums[i]);

    int sum = 0;

    for(int i = 0; i < n; i++) {
        for(int j = i; j < n; j++) {
            int freq[1000] = {0};  // Frequency array (assuming elements â‰¤ 1000)
            int distinct = 0;

            for(int k = i; k <= j; k++) {
                if(freq[nums[k]] == 0)
                    distinct++;
                freq[nums[k]]++;
            }

            sum += distinct * distinct; // Add square of distinct count
        }
    }

    printf("Sum of squares of distinct counts = %d\n", sum);
    return 0;
}
