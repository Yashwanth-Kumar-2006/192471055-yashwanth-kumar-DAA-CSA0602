#include <stdio.h>
#include <string.h>

int isPalindrome(char *s) {
    int i=0, j=strlen(s)-1;
    while(i<j) if(s[i++]!=s[j--]) return 0;
    return 1;
}

int main() {
    char *words[] = {"abc","car","ada","racecar","cool"};
    int n = sizeof(words)/sizeof(words[0]);
    for(int i=0;i<n;i++){
        if(isPalindrome(words[i])){
            printf("%s", words[i]);
            return 0;
        }
    }
    printf("");
    return 0;
}
